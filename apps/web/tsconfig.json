{
  "extends": "@melo/ts-config/nextjs.json",
  "compilerOptions": {
    "paths": {
      "@/web/*": ["./src/*"],
      "@/web/env": ["./src/env/index"],
      "react": [ "./node_modules/@types/react" ]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}